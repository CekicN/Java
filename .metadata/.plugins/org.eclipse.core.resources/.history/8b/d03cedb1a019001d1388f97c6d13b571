package algebra;

public class Razlomak extends Broj{

	private int br,im;
	
	public void ucitaj(String b,String i)
	{
		br = super.ucitaj(b);
		im = super.ucitaj(i);
	}
	@Override
	public int compareTo(Broj o) {
		
		return 0;
	}

	@Override
	public double vrednost() {
		
		return br/(double)im;
	}

}
